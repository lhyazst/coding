  <select id="selectBagUserNum"  resultType="java.lang.Long">
    SELECT COUNT(bags_id) FROM warehouse_bags b  WHERE   warehouse_id
    IN(SELECT orc_id FROM warehouse w WHERE w.warehouse_id = #{id,jdbcType=BIGINT})
    AND DATE(create_time) =  #{date,jdbcType=VARCHAR} AND b.`bags_id`IN(SELECT id FROM bags WHERE STATUS = "2")

  </select>

